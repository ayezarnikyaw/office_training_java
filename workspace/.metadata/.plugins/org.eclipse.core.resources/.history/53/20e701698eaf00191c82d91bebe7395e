package pack;

public class Account {

	private int acc_no;

	public void setAcc_no(int acc_no) {
		// TODO Auto-generated method stub
		this.acc_no = acc_no;
	}

	public int getAcc_no() {
		// TODO Auto-generated method stub
		return acc_no; 
	}

	public void setAcc_Name(String acc_name) {
		// TODO Auto-generated method stub
		this.acc_name = acc_name;
		
	}

}
